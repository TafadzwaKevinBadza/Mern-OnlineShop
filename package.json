    "url": "https://github.com/KevinBadza/Mern-OnlineShop/issues"
  },
  "homepage": "https://github.com/KevinBadza/Mern-OnlineShop#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.15",
    "redux-devtools-extension": "^2.13.9",
    "require": "^2.4.20"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
